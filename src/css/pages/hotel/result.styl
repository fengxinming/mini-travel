.result-wrapper
  background #fff
  height 100%
  .title
    font-size px2rpx(44px)
    color #3b4f62
    font-weight 600
    padding-top px2rpx(280px)
    text-align center
    background url($imgUrlPrefix+'/images/hotel/success.png') no-repeat bottom px2rpx(70px) center
    background-size px2rpx(80px)
  .text
    margin-top px2rpx(20px)
    font-size px2rpx(30px)
    line-height px2rpx(42px)
    color #9da4b5
    text-align center
  .btns
    margin-top px2rpx(90px)
    .btn
      width px2rpx(254px)
      height px2rpx(80px)
      line-height @height
      text-align center
      border-radius px2rpx(9999px)
      margin 0 auto
      font-size px2rpx(30px)
      margin-bottom px2rpx(35px)
      &.go-detail
        background #0CC071
        color #fff
      &.go-booking
        background #fff
        color #0CC071
        border px2rpx(1px) solid #0CC071